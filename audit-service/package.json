    "nodemon": "^2.0.15",
    "pg": "^8.7.1",
    "request-parser": "file:core/request-parser",
    "response-handler": "file:core/response-handler",
    "sequelize": "^6.14.1",
    "sequelize-cli": "^6.4.1",
    "spec": "^1.0.1",
    "sqs-consumer": "^5.6.0",
    "uuid": "^8.3.2",
    "winston": "^3.4.0"
