{
  "name": "audit-service",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "db:migrate": "set -a && . ./.env && ./node_modules/sequelize-cli/lib/sequelize db:migrate --config ./config/database.js",
    "db:test:migrate": "NODE_ENV=test npm run db:migrate",
    "db:all:migrate": "set -a && . ./.env && ./node_modules/sequelize-cli/lib/sequelize db:migrate --config ./config/database.js",
    "db:prod:all:migrate": "./node_modules/sequelize-cli/lib/sequelize db:migrate --config ./config/database.js",
    "db:migrate:undo": "./node_modules/sequelize-cli/lib/sequelize db:migrate:undo --config ./config/database.js",
    "db:test:migrate:undo": "NODE_ENV=test npm run db:migrate:undo",
    "db:all:migrate:undo": "npm run db:migrate:undo;npm run db:test:migrate:undo",
    "start:dev": "./node_modules/nodemon/bin/nodemon.js --inspect=0.0.0.0:56769 ./bin/www",
    "start": "set -a && . ./.env && ./node_modules/nodemon/bin/nodemon.js ./bin/www",
    "lint": "node_modules/eslint/bin/eslint.js .",
    "auto:lint:fix": "eslint --fix --ext .js,.jsx .",
    "tests": "npx mocha --reporter spec"
  },
  "dependencies": {
    "babel-eslint": "^10.1.0",
    "body-parser": "^1.19.1",
    "chai": "^4.3.6",
    "debug": "~2.6.9",
    "errors": "file:core/errors",
    "eslint": "^8.8.0",
    "eslint-plugin-import": "^2.25.4",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "logger": "file:core/logger",
    "mocha": "^9.2.2",
    "moment-timezone": "^0.5.34",
